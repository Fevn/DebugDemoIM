<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <attr format="color|reference" name="topbar_background"/>
    <color name="black">#FF000000</color>
    <color name="color_3">#333333</color>
    <color name="color_777575">#777575</color>
    <color name="color_808080">#808080</color>
    <color name="color_999999">#999999</color>
    <color name="color_ece8e9">#ECE8E9</color>
    <color name="color_ff0000">#ff0000</color>
    <color name="divider">#eeeef1</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="topbar_background_color">@android:color/white</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">RcDemoIM</string>
    <string name="rc_conversation_group">群聊</string>
    <string name="rc_conversation_private">单聊</string>
    <string name="rc_conversation_system">系统</string>
    <string name="rc_conversationlist_text_parameter">配置会话列表页面参数</string>
    <string name="rc_conversationlist_text_parameter_title">会话列表页面参数</string>
    <string name="rc_conversationlist_tigs">如果不配置，本Demo默认展示单聊、群聊和系统会话，并且将系统会话聚合。</string>
    <string name="rc_login_text_login">登录</string>
    <string name="rc_setting_text">配置</string>
    <string name="rc_setting_text_navigation_file">配置导航和文件服务</string>
    <string name="rc_setting_text_navigation_file_title">导航和文件服务</string>
    <string name="rc_setting_text_push_language_title">推送多语言</string>
    <string name="text_save">保存</string>
    <style name="LineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>
    <style name="TextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">12dp</item>
    </style>
    <style name="Theme.RcDemoIM" parent="Theme.AppCompat.Light.NoActionBar">
        
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        
    </style>
    <declare-styleable name="BottomTabView">
        <attr format="reference" name="tabIconNormal"/>
        <attr format="string" name="tabIconNormalString"/>
        <attr format="reference" name="tabIconSelected"/>
        <attr format="string" name="tabIconSelectedString"/>
        <attr format="color" name="tabTextColorNormal"/>
        <attr format="color" name="tabTextColorSelected"/>
        <attr format="string" name="tabText"/>
        <attr format="dimension" name="tabTextSize"/>
        <attr format="dimension" name="tabIconWidth"/>
        <attr format="dimension" name="tabIconHeight"/>
    </declare-styleable>
    <declare-styleable name="InputTextLinearLayout">
        <attr format="string" name="leftText"/>
    </declare-styleable>
    <declare-styleable name="TopBar">
        <attr format="string" name="centerText"/>
        <attr format="string" name="rightText"/>
        <attr format="boolean" name="hideBack"/>
        <attr format="boolean" name="is_add_more"/>
        <attr format="reference" name="right_add_icon"/>
        <attr format="reference" name="rightIcon"/>
        <attr format="integer" name="centerTextSize"/>
        <attr format="color|reference" name="right_textColor"/>
        <attr format="reference" name="top_bar_background"/>
        <attr format="boolean" name="isShowDivider"/>
        <attr format="reference" name="finishimage"/>
    </declare-styleable>
</resources>